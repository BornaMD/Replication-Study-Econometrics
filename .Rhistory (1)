#Libraries
library(haven)
library(carData)
library(car)
library(foreign)
library(dplyr)
library(ggplot2)
#Loading Data
data4 <- read_stata("paper4.dta")
attach(data4)
# Defining Functions
sum_s <- function(x) {
return(c((length(na.omit(x))) ,
(sd(na.omit(x)))
(summary(na.omit(x)))))
}
regr_s <- function(x) {
return(c((x),
(summary(x)),
(lenght(x$fitted.values)),
(x$coefficients)))
}
sum_s(data4)
summary(data4)
sd(data4$agriculture_top)
summary(data4$time_weighted_eucldist)
View(data4)
View(data4)
summary(data4$rain_sd)
