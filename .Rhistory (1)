#Loading Data
data4 <- read_stata("paper4.dta")
attach(data4)
# Defining Functions
part_I <- function(x) {
return(c((length(na.omit(x))) ,
(sd(na.omit(x)))
(mean(na.omit(x)))))
}
sum_s <- function(x) {
return(c((length(na.omit(x))) ,
(sd(na.omit(x)))
(summary(na.omit(x)))))
}
regr_s <- function(x) {
return(c((x),
(summary(x)),
(lenght(x$fitted.values)),
(x$coefficients)))
}
