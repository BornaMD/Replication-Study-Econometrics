# Defining Functions
part_I <- function(x) {
return(c((length(na.omit(x))) ,
(sd(na.omit(x)))
(mean(na.omit(x)))))
}
part_I(data4$agriculture_top)
part_I(data4$selfemployment_top)
part_I(data4$employee_top)
part_I(data4$time_eucldist_bu)
part_I(data4$time_eucldist_rw)
part_I(data4$ageyr)
part_I(data4$female)
part_I(data4$head)
