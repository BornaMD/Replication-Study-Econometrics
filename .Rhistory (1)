# Defining Functions
sum_s <- function(x) {
return(nobs(na.omit(x)))
return(sd(na.omit(x)))
return(summary(na.omit(x)))
}
regr_s <- function(x) {
return(x)
return(summary(x))
return(lenght(x$fitted.values))
return(x$coefficients)
}
sum_s(data$ageyr)
